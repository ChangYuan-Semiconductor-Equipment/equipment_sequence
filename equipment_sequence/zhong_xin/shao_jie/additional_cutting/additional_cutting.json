                "address": "Application.gvl_OPMODE01_MES.RecipeID",
                "data_type": "int"
            },
            "new_lot": {
                "description": "开工单",
                "call_backs": [
                    {
                        "description": "写入工单状态, 开工单成功",
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.geneal.Lot.Cmd",
                        "data_type": "int",
                        "value": 1
                    },
                    {
                        "description": "等待一秒",
                        "operation_func": "wait_one_second"
                    },
                    {
                        "description": "写入EAP下发的工单号",
                        "operation_func": "write_dv_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.geneal.Lot.Name",
                        "data_type": "string",
                        "sv_name": "circulate_name"
                    },
                    {
                        "description": "写入EAP下发的工单数量",
                        "operation_func": "write_sv_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.geneal.Lot.QuantitySetting",
                        "data_type": "int",
                        "sv_name": "lot_quantity"
                    },
                    {
                        "description": "写入工单状态, 允许运行",
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.geneal.Lot.Cmd",
                        "data_type": "int",
                        "value": 3
                    }
                ]
            },
            "end_lot": {
                "description": "结工单",
                "call_backs": [
                    {
                        "description": "写入工单状态2",
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.geneal.Lot.Cmd",
                        "data_type": "int",
                        "value": 2,
                        "event_name": "end_lot"
                    }
                ]
            },
            "pp_select": {
                "description": "切换配方",
                "call_backs": [
                    {
                        "description": "写入要切换的配方id",
                        "operation_func": "write_sv_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.changeOver.recipeID",
                        "data_type": "int",
                        "sv_name": "pp_select_recipe_id"
                    },
                    {
                        "description": "写入要切换的配方指令",
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.changeOver.CMD",
                        "data_type": "int",
                        "value": 2
                    },
                    {
                        "description": "清空 通知 plc 切换配方信号",
                        "premise_address": "Application.gvl_OPMODE01_MES.plc2mes.changeOver.CMD",
                        "premise_value": 2,
                        "premise_data_type": "int",
                        "premise_time_out": 3,
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.changeOver.CMD",
                        "data_type": "int",
                        "value": 0
                    },
                    {
                        "description": "读取设备当前配方id",
                        "operation_func": "read_update_sv",
                        "address": "Application.gvl_OPMODE01_MES.RecipeID",
                        "data_type": "int",
                        "sv_name": "current_recipe_id"
                    }
                ]
            },
            "solder_carrier_in_request_uploading": {
                "loop": true,
                "description": "焊接托盘请求进入pinfin设备事件",
                "address": "Application.gvl_OPMODE01_MES.mes2plc.TrackIn.Resp",
                "data_type": "int",
                "value": 1,
                "call_backs": [
                    {
                        "description": "清除 eap 回复了焊接托盘进站请求标识",
                        "operation_func": "clean_eap_reply",
                        "dv_name": "is_eap_reply_carrier_in_uploading"
                    },
                    {
                        "description": "读取焊接托盘码",
                        "operation_func": "read_update_dv",
                        "address": "Application.gvl_OPMODE01_MES.plc2mes.TrackIn.code",
                        "data_type": "string",
                        "dv_name": "solder_carrier_code_in_pinfin",
                        "event_name": "solder_carrier_in_request_uploading"
                    },
                    {
                        "description": "焊接托盘出站删除此焊接托盘的所有信息",
                        "operation_func": "delete_database_data_solder_carrier_out_cutting"
                    },
                    {
                        "description": "等待 eap 回复焊接托盘是否允许进站",
                        "operation_func": "wait_eap_reply",
                        "dv_name": "is_eap_reply_carrier_in_uploading",
                        "not_allow_dv_name": "is_allow_solder_carrier_in_uploading",
                        "not_allow_dv_value": 1
                    },
                    {
                        "description": "写入 eap 是否允许焊接托盘进站",
                        "operation_func": "write_dv_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.TrackIn.Status",
                        "data_type": "int",
                        "dv_name": "is_allow_solder_carrier_in_uploading"
                    },
                    {
                        "description": "写入焊接托盘的穴位状态",
                        "operation_func": "write_multiple_dv_value_tag",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.TrackIn.Mapping[$].status",
                        "data_type": "int",
                        "dv_name": "solder_carrier_index_states"
                    },
                    {
                        "description": "MES 清空焊接托盘请求进入pinfin设备事件请求信号, 写入 -1",
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.TrackIn.Resp",
                        "data_type": "int",
                        "value": -1
                    }
                ]
            },
            "product_in_request": {
                "loop": true,
                "description": "上料机基板产品请求进站事件",
                "address": "Application.gvl_OPMODE01_MES.mes2plc.Interlock[2].Resp",
                "data_type": "int",
                "value": 1,
                "call_backs": [
                    {
                        "description": "清除 eap 回复了基板产品进站请求标识",
                        "operation_func": "clean_eap_reply",
                        "dv_name": "is_eap_reply_product_in_uploading"
                    },
                    {
                        "description": "读取基板产品码",
                        "operation_func": "read_update_dv",
                        "address": "Application.gvl_OPMODE01_MES.plc2mes.Interlock[2].code",
                        "data_type": "string",
                        "dv_name": "product_code_pinfin",
                        "event_name": "product_in_request"
                    },
                    {
                        "description": "等待 eap 回复基板产品进站请求",
                        "operation_func": "wait_eap_reply",
                        "dv_name": "is_eap_reply_product_in_uploading",
                        "not_allow_dv_name": "is_allow_product_in_uploading",
                        "not_allow_dv_value": 2
                    },
                    {
                        "description": "写入 eap 是否允许基板产品进站",
                        "operation_func": "write_dv_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.Interlock[2].Status",
                        "data_type": "int",
                        "dv_name": "is_allow_product_in_uploading"
                    },
                    {
                        "description": "MES 清空上料机基板产品请求进站事件请求信号, 写入 -1",
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.Interlock[2].Resp",
                        "data_type": "int",
                        "value": -1
                    }
                ]
            },
            "solder_carrier_out_uploading": {
                "loop": true,
                "description": "上料机焊接托盘流出",
                "address": "Application.gvl_OPMODE01_MES.mes2plc.TrackOut",
                "data_type": "int",
                "value": 1,
                "call_backs": [
                    {
                        "description": "设置焊接托盘从上料机出去的时间",
                        "operation_func": "set_time_dv_value",
                        "dv_name": "solder_carrier_out_time_uploading"
                    },
                    {
                        "description": "读取上料机焊接托盘出站时的焊接托盘码",
                        "operation_func": "read_update_dv",
                        "address": "Application.gvl_OPMODE01_MES.plc2mes.TrackOut.Result[1].code[0]",
                        "data_type": "string",
                        "dv_name": "solder_carrier_code_out_uploading"
                    },
                    {
                        "description": "读取上料机焊接托盘出站时里面所有的基板产品码",
                        "operation_func": "read_multiple_update_dv_tag",
                        "address": "Application.gvl_OPMODE01_MES.plc2mes.TrackOut.Result[$].code[5]",
                        "count_num": 2,
                        "data_type": "string",
                        "dv_name": "product_code_list_uploading"
                    },
                    {
                        "description": "读取上料机焊接托盘出站时里面所有的治具码",
                        "operation_func": "read_multiple_update_dv_tag",
                        "address": "Application.gvl_OPMODE01_MES.plc2mes.TrackOut.Result[1].code[7]",
                        "count_num": 2,
                        "data_type": "string",
                        "dv_name": "solder_jig_code_list",
                        "event_name": "solder_carrier_out_uploading"
                    },
                    {
                        "description": "MES 清空上料机焊接托盘流出信号, 写入 -1",
                        "operation_func": "write_specify_value",
                        "address": "Application.gvl_OPMODE01_MES.mes2plc.TrackOut",
                        "data_type": "int",
                        "value": -1
                    },
